{
  "name": "exutile",
  "version": "1.0.4",
  "description": "A utility library designed to simplify and enhance Express.js applications.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "LICENSE"
  ],
  "engines": {
    "node": ">=22.13.1"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"./src/**/*.{ts,js}\"",
    "lint": "eslint \"./src/**/*.{ts,js}\" --fix",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aashishpanchal/exutile.git"
  },
  "author": "Aashish Panchal <aipanchal51@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.6",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "prettier": "^3.5.2",
    "rimraf": "^6.0.1",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.26.1"
  },
  "keywords": [
    "proxy",
    "express",
    "exutile",
    "handler",
    "utility",
    "http-status",
    "http-errors",
    "asyncHandler"
  ],
  "homepage": "https://github.com/aashishpanchal/exutile#readme",
  "packageManager": "bun@1.2.2"
}
